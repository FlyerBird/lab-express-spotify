<div class="title-listen">
   <h2>Title</h2>
   <h2>Listen</h2>
</div>

<div class="tracklist">
<ul>
    {{#each items}}
    <li>
        <h3>{{name}}</h3>
        {{#if preview_url}}

<figure>
        <figcaption>Listen to {{name}}:</figcaption>
    <audio
        controls
        src="{{preview_url}}">
            Your browser does not support the
            <code>audio</code> element.
    </audio>
</figure>
    {{else}}
    <p>No audio available amigo</p>
    {{/if}}
    </li>
    {{/each}}
</ul>
</div>

